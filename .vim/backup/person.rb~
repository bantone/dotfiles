class Person
    def dob(y)
        @mdy = y
    end
        def dob=(y)
            
    def age(x)
        @age = x
    end
        def age=29
    
    def firstname(first)
        @first = first
    end

        def firstname=(brandon)

    def surname(last)
        @last = last

    end     
        def surname=(antone)
    end
end


brandon = Person.new
brandon.dob = "'10/19/1983"
brandon.age = "29"
brandon.firstname = "brandon"
brandon.surname = "antone"

puts brandon.firstname
puts brandon.surname
puts brandon.dob 
puts brandon.age

end
